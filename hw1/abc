public boolean canJump (int[] array){
        if(array == null || array.length == 0) return false;
        int maxStep = 0;
        for(int i = 0; i < maxStep; i++){
            maxStep = Math.max(max, i + array[i]);
            if(maxStep >= array.length - 1)
                return true;
        }
        return false;
}